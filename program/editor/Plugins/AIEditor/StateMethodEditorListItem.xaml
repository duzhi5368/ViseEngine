<UserControl x:Class="AIEditor.StateMethodEditorListItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" d:DesignWidth="392.5">
	<UserControl.Resources>
		<Image x:Key="DeactiveLinksImage" Source="Source/links_D.png"/>
		<Image x:Key="ActiveLinksImage" Source="Source/links.png"/>
	</UserControl.Resources>
    <Grid>
    	<Grid.ColumnDefinitions>
    		<ColumnDefinition Width="0.953*"/>
    		<ColumnDefinition Width="Auto"/>
    	</Grid.ColumnDefinitions>
    	<TextBlock x:Name="TextBlock_MethodName" HorizontalAlignment="Left" Margin="5,2,0,2" TextWrapping="Wrap" Text="TextBlock" Foreground="White" VerticalAlignment="Center"/>
    	<Image x:Name="Image_Links" Source="Source/links_D.png" Margin="2" Grid.Column="1" Width="16" Height="16" />
            
    </Grid>
</UserControl>
